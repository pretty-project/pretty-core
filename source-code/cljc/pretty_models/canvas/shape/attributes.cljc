(defn shape-canvas-body-attributes
  ; @description
  ; Associates the body element attributes of the shape canvas model to the given attribute map.
  ;
  ; @param (map) attributes
  ; @param (map) props
  ;
  ; @return (map)
  [attributes _]
  (-> attributes))

